<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<VBox>
		<Select items="{/cities}">
			<items>
				<core:Item text="{name}" key="{name}"></core:Item>
			</items>
		</Select>
		<ComboBox items="{/cities}">
			<items>
				<core:Item text="{name}" key="{name}"></core:Item>
			</items>
		</ComboBox>
		<MultiComboBox items="{/cities}">
			<items>
				<core:Item text="{name}" key="{name}"></core:Item>
			</items>
		</MultiComboBox>
	</VBox>
</core:FragmentDefinition>